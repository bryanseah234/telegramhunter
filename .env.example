# General Configuration
ENV=production
DEBUG=False

# Redis
REDIS_URL=redis://redis:6379/0

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-semcret-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Telegram Monitoring Bot (Broadcaster)
MONITOR_BOT_TOKEN=your-bot-token
MONITOR_GROUP_ID=@yourgroupname
WHITELISTED_BOT_IDS=1234567,9876543,admin_bot # Optional: Bots to keep in group

# Telethon (Scraper)
TELEGRAM_API_ID=123456
TELEGRAM_API_HASH=your-api-hash

# OSINT Keys
GITHUB_TOKEN=
SHODAN_API_KEY=
FOFA_EMAIL=
FOFA_KEY=
URLSCAN_KEY=
CENSYS_ID=
HYBRID_ANALYSIS_KEY=

# ========== GITHUB ACTIONS SECRETS (Not in .env, add to GitHub) ==========
# These are for automated deployments, NOT for local .env file.
# Add them to: GitHub Repo Settings > Secrets and variables > Actions

# Supabase Keep-Alive Workflow
# SUPABASE_URL=
# SUPABASE_KEY=

# Oracle Cloud Auto-Deploy Workflow
# ORACLE_HOST=your-oracle-vm-public-ip
# ORACLE_USERNAME=ubuntu
# ORACLE_KEY=<contents of your private SSH key file>

# ========== VERCEL ENV VARS (Add in Vercel Dashboard) ==========
# NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
# NEXT_PUBLIC_SUPABASE_KEY=your-supabase-anon-key

